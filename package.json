{
  "name": "saas-plat-server",
  "description": "saasplat app server",
  "version": "1.0.11",
  "scripts": {
    "start": "node   config/testing.js",
    "debug": "node-debug --web-port=9900 --no-preload config/production.js",
    "compile": "babel src/ --out-dir lib/ --source-maps",
    "watch": "babel --watch src/ --out-dir lib/ --source-maps",
    "test": "node node_modules/mocha/bin/mocha --timeout 300000 --compilers js:babel-core/register ",
    "test-all": "node node_modules/mocha/bin/mocha --timeout 300000 --compilers js:babel-core/register test/*_test.js",
    "test-debug": "node-debug   --web-port=9900  node_modules/mocha/bin/_mocha --timeout 300000  --compilers js:babel-core/register ",
    "pub": "npm publish --registry http://registry.npmjs.org",
    "prepublish": "npm run compile",
    "install-modules": "node config/install"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-polyfill": "^6.13.0",
    "babel-register": "^6.24.1",
    "cqrs-fx": "^1.0.6",
    "glob": "^7.1.1",
    "log4js": "^1.1.1",
    "moment": "^2.18.1",
    "mysql": "^2.10.2",
    "sequelize": "^3.30.4",
    "socket.io": "^1.3.7",
    "thinkjs": "2.2.x"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "6.x.x",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.3.0",
    "source-map-support": "0.4.0"
  },
  "repository": "https://github.com/saas-plat/saas-plat-server",
  "license": "GPL-3.0"
}
