{
  "name": "saas-plat-server",
  "description": "saasplat base server",
  "version": "1.0.0",
  "scripts": {
    "start": "node config/production.js",
    "debug": "node-debug --web-port=9900 --no-preload config/production.js",
    "compile-thinkjs": "babel --presets es2015,stage-1 --plugins transform-runtime thinkjs/src/ --out-dir thinkjs/app/ --source-maps",
    "watch-compile-thinkjs": "babel --watch --presets es2015,stage-0,stage-1,stage-2,stage-3 --plugins transform-runtime thinkjs/src/ --out-dir thinkjs/app/ --source-maps",
    "compile": "babel --presets es2015,stage-1 --plugins transform-runtime src/ --out-dir app/ --source-maps",
    "watch-compile": "babel --watch --presets es2015,stage-0,stage-1,stage-2,stage-3 --plugins transform-runtime src/ --out-dir app/ --source-maps",
    "watch": "npm run watch-compile && npm run watch-compile-thinkjs",
    "prepublish": "npm run compile && npm run compile-thinkjs"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "6.x.x",
    "co": "^4.6.0",
    "cqrs": "github:saas-plat/cqrs",
    "log4js": "^0.6.38",
    "mysql": "^2.10.2",
    "sequelize": "^3.21.0",
    "source-map-support": "0.4.0",
    "thinkjs": "2.2.x"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-core": "6.x.x"
  },
  "repository": "http://git.oschina.net/saas-plat/saas-plat-server",
  "license": "MIT",
  "modules":[],
  "eventdb": {
    "username": "root",
    "password": "123456",
    "database": "testserver1_events",
    "host": "localhost",
    "dialect": "mysql"
  },
  "querydb": {
    "username": "root",
    "password": "123456",
    "database": "testserver1_querys",
    "host": "localhost",
    "dialect": "mysql"
  }
}
