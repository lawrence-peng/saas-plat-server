{
  "name": "saas-plat-server",
  "description": "saasplat base server",
  "version": "1.0.0",
  "scripts": {
    "start": "node config/production.js",
    "debug": "node-debug --web-port=9900 --no-preload config/production.js",
    "compile-thinkjs": "babel thinkjs/src/ --out-dir thinkjs/app/ --source-maps",
    "watch-compile-thinkjs": "babel --watch thinkjs/src/ --out-dir thinkjs/app/ --source-maps",
    "compile": "babel src/ --out-dir app/ --source-maps",
    "watch-compile": "babel --watch src/ --out-dir app/ --source-maps",
    "test": "node node_modules/mocha/bin/mocha --compilers js:babel-core/register ",
    "test-all": "node node_modules/mocha/bin/mocha --compilers js:babel-core/register test/*_test.js",
    "test-debug": "node-debug   --web-port=9900  node_modules/mocha/bin/_mocha   --compilers js:babel-core/register ",
    "prepublish": "npm run compile && npm run compile-thinkjs"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-polyfill": "^6.13.0",
    "co": "^4.6.0",
    "cqrs-fx": "^1.0.2",
    "glob": "^7.1.1",
    "log4js": "^0.6.38",
    "mysql": "^2.10.2",
    "sequelize": "^3.21.0",
    "thinkjs": "2.2.x"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "6.x.x",
    "source-map-support": "0.4.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "babel-core": "6.x.x"
  },
  "repository": "http://git.oschina.net/saas-plat/saas-plat-server",
  "license": "Apache License",
  "modules": "saas-plat-*",
  "eventdb": {
    "username": "root",
    "password": "123456",
    "database": "testserver1_events",
    "host": "localhost",
    "dialect": "mysql"
  },
  "querydb": {
    "username": "root",
    "password": "123456",
    "database": "testserver1_querys",
    "host": "localhost",
    "dialect": "mysql"
  }
}
